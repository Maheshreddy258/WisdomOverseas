<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wisdomoverseas"
    android:targetSandboxVersion="2"
    android:versionCode="26"
    android:versionName="1.25" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="28" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_CALL_LOG" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:icon="@drawable/logo2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true" >
        <activity android:name="com.example.mahesh.wisdomoverseas.activities.LeadStatusActivity" />
        <activity android:name="com.example.mahesh.wisdomoverseas.activities.BlankActivity" />
        <activity android:name="com.example.mahesh.wisdomoverseas.activities.uploadActivity" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.SearchActivity2"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.Notification"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.SearchActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.RecordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.IncomingcallsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.CallBacksActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.IntrestedCallsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.PendingRemindersActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.ReminderNewInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.NewInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.RemainderActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.HistoryActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.InfoReminder"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.SplashActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.LoginActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.forgotpwdActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.DashBoardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.TrackStatus"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.ProfileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.InfoActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.TodayCalls"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mahesh.wisdomoverseas.activities.Report"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name="com.nbsp.materialfilepicker.ui.FilePickerActivity"
            android:theme="@style/MFP_BaseTheme" >
        </activity>
    </application>

</manifest>